<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Jedzonko" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:background">#faf6f3</item>
        <item name="font">ℂ𝕫𝕔𝕚𝕠𝕟𝕜𝕚</item>
        <item name="android:textColor">#707070</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">17dp</item>
    </style>
    <style name="przyciski">
        <item name="android:shape">oval</item>
        <item name="android:radius">360dp</item>
        <item name="android:backgroundTint">#FAEED6</item>
        <item name="android:textColor">#707070</item>
    </style>
</resources>